import test.TestCase;

class Blah{
   const x;
   new(x){
      this.x=x;
   }
}

class ListTest with TestCase {

   testList() {
      var v1 = new Blah(1);
      var v2 = new Blah(2);
      var v3 = new Blah(3);
      var list1 = [];
      var list2 = [];
      
      assert v1.equals(v1);
      assert !v1.equals(v2);
      assert v2.equals(v2);
      assert !v2.equals(v3);
      assert v3.equals(v3);
      
      list1.add(v1);
      list1.add(v2);
      list1.add(v3);
      list2.add(v2);
      
      assert list2.length == 1;
      assert list1.length == 3;
      assert list1.contains(v1);
      assert list1.contains(v2);
      assert list1.contains(v3);
      assert list2.contains(v2);
      
      list1.removeAll(list2);
   
      assert list2.length == 1;
      assert list1.length == 2;
      assert list1.contains(v1);
      assert !list1.contains(v2);
      assert list1.contains(v3);
      assert list2.contains(v2);
   }
}
