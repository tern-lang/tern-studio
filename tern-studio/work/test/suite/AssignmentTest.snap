import test.TestCase;

class Blah{
   const x;
   const y;
   const z;
   new(a,b,c){
      this.x=(a+b)*c;
      y="${a}+${b}";
      this.z=-c;
   }
   toString(){
      return "x=${x}, y=${y}, z=${z}";
   }
}

class AssignmentTest with TestCase {

   testAssignments() {
      var b= new Blah(1,2,3);
      assert b.x == (1+2)*3;
      assert b.y == "1+2";
      assert b.z == -3;
      println(b);
   }
}
