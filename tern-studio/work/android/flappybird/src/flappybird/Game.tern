
/**
 * The entry point into the game. We'll simply create an
 * instance of class which will start the display and game
 * loop.
 */
func start(game) {
   let p = new GamePanel(context);

   // Start the game at 20 frames per second
   try {
      game.start(p, 20);
   } catch(e) {
      e.printStackTrace();
   }
}
   
start(game);