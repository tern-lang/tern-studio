assert duplicateNum([1, 2, 3, 4, 2, 5, 7, 8, 9]);

func duplicateNum(nums) {
   Collections.sort(nums);
   
   for(i in 1 to nums.length -1){
      if(nums[i -1] == nums[i]){
         nums[i] = 0;
      }
   }
   println(nums);
}
