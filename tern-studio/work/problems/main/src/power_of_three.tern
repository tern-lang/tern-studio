// https://leetcode.com/problems/power-of-three/

assert powerOfThree(27) == true;
assert powerOfThree(0) == false;
assert powerOfThree(9) == true;

func powerOfThree(num) {
   loop {
      if(num < 3) {
         return false;
      }
      num = num / 3;
      
      if(num == 1) {
         return true;
      }
   }
}