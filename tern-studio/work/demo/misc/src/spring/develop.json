{
   "Workspace": {
      "class": "develop.Workspace",
      "init-method": "create",
      "constructor-args": [
         "${directory}"
      ]
   },
   "BackupManager": {
      "class": "develop.BackupManager",
      "constructor-args": [
         "ConsoleLogger",
         "Workspace"
      ]
   },
   "ConfigurationReader": {
      "class": "develop.configuration.ConfigurationReader",
      "constructor-args": [
         "ConsoleLogger",
         "Workspace"
      ]
   },
   "ConfigurationClassLoader": {
      "class": "develop.configuration.ConfigurationClassLoader",
      "constructor-args": [
         "ConfigurationReader"
      ]
   },
   "ProcessConfigurationLoader": {
      "class": "develop.configuration.ProcessConfigurationLoader",
      "constructor-args": [
         "ConfigurationReader"
      ]
   },
   "ConsoleLogger": {
      "class": "agent.ConsoleLogger",
      "constructor-args": [
         "${verbose}"
      ]
   },
   "ProcessManager": {
      "class": "develop.ProcessManager",
      "constructor-args": [
         "ProcessConfigurationLoader",
         "ConsoleLogger",
         "Workspace",
         "${agent-port}",
         "${agent-pool}"
      ]
   },
   "ProcessServer": {
      "class": "develop.ProcessServer",
      "init-method": "start",
      "constructor-args": [
         "ProcessManager",
         "WebServer"
      ]
   },
   "ConnectListener": {
      "class": "develop.ConnectListener",
      "constructor-args": [
         "ProjectBuilder"
      ]
   },
   "ContentTypeResolver": {
      "class": "develop.http.resource.ContentTypeResolver",
      "constructor-args": [
         {
            ".*.vm": "text/html",
            ".*.html": "text/html",
            ".*.png": "image/png",
            ".*.jpg": "image/png",
            ".*.gif": "image/gif",
            ".*.css": "text/css",
            ".*.js": "text/javascript",
            ".*.json": "application/json",
            ".*.tern": "text/plain"
         }
      ]
   },
   "FileManager": {
      "class": "develop.http.resource.FileManager",
      "constructor-args": [
         "/resource"
      ]
   },
   "FileResolver": {
      "class": "develop.http.resource.FileResolver",
      "constructor-args": [
         "FileManager"
      ]
   },
   "VelocityEngine": {
      "class": "org.apache.velocity.app.VelocityEngine",
      "init-method": "init",
      "constructor-args": [
         {
            "runtime.log.logsystem.class": "org.apache.velocity.runtime.log.Log4JLogChute",
            "runtime.log.logsystem.log4j.logger": "root",
            "parser.pool.size": "5"
         }
      ]
   },
   "TemplateEngine": {
      "class": "develop.http.resource.template.VelocityTemplateEngine",
      "constructor-args": [
         "VelocityEngine",
         "FileResolver",
         "/",
         ".vm"
      ]
   },
   "TypeScriptCompiler": {
      "class": "develop.compile.TypeScriptCompiler",
      "constructor-args": [
         "src/main/typescript/tsc.js",
         "C:/Program Files/nodejs/node.exe"
      ]
   },
   "TypeScriptResource": {
      "class": "develop.compile.TypeScriptResource",
      "constructor-args": [
         "TypeScriptCompiler",
         "FileMatcher",
         "src/main/resources/resource/ts",
         [
            "src/main/resources/resource/js",
            "target/classes/resource/js"
         ]
      ]
   },
   "CompletionResource": {
      "class": "develop.complete.CompletionResource",
      "constructor-args": [
         "ProjectBuilder",
         "ConfigurationClassLoader",
         "ConsoleLogger"
      ]
   },
   "TypeNodeResource": {
      "class": "develop.complete.TypeNodeResource",
      "constructor-args": [
         "ProjectBuilder",
         "ConfigurationClassLoader",
         "ConsoleLogger"
      ]
   },
   "SourceFormatResource": {
      "class": "develop.complete.SourceFormatResource",
      "constructor-args": [
         "ProjectBuilder"
      ]
   },
   "ProjectBuilder": {
      "class": "develop.http.project.ProjectBuilder",
      "constructor-args": [
         "Workspace",
         "${project-mode}"
      ]
   },
   "ProjectResource": {
      "class": "develop.http.project.ProjectResource",
      "constructor-args": [
         "TemplateEngine",
         "project.vm"
      ]
   },
   "ProjectOpenDialog": {
      "class": "develop.http.project.ProjectOpenDialog",
      "constructor-args": [
         "ProjectBuilder",
         "TemplateEngine",
         "start.vm"
      ]
   },
   "ProjectFileResource": {
      "class": "develop.http.project.ProjectFileResource",
      "constructor-args": [
         "ProjectBuilder",
         "ContentTypeResolver"
      ]
   },
   "ProjectTreeResource": {
      "class": "develop.http.project.ProjectTreeResource",
      "constructor-args": [
         "ProjectBuilder"
      ]
   },
   "ProjectDisplayResource": {
      "class": "develop.http.project.ProjectDisplayResource",
      "constructor-args": [
         "ProjectBuilder",
         ".display"
      ]
   },
   "ProjectScriptService": {
      "class": "develop.http.project.ProjectScriptService",
      "constructor-args": [
         "ProjectBuilder",
         "ConnectListener",
         "ConfigurationClassLoader",
         "ConsoleLogger",
         "ProcessManager",
         "BackupManager"
      ]
   },
   "ClassResourceLoader": {
      "class": "develop.http.loader.ClassResourceLoader",
      "constructor-args": [
         ""
      ]
   },
   "ClassLoaderResource": {
      "class": "develop.http.loader.ClassLoaderResource",
      "constructor-args": [
         "ClassResourceLoader",
         "ConsoleLogger",
         "${verbose}"
      ]
   },
   "RemoteProcessBuilder": {
      "class": "develop.http.loader.RemoteProcessBuilder",
      "init-method": "create",
      "constructor-args": [
         "ClassResourceLoader",
         "Workspace"
      ]
   },
   "NotFoundResource": {
      "class": "develop.http.resource.StringResource",
      "constructor-args": [
         "Resource could not be found!!",
         "text/plain; charset=UTF-8",
         "UTF-8",
         "NOT_FOUND"
      ]
   },
   "ResourceMatcher": {
      "class": "develop.http.resource.RegularExpressionMatcher",
      "constructor-args": [
         {
            "/format.*": "SourceFormatResource",
            "/type.*": "TypeNodeResource",
            "/complete.*": "CompletionResource",
            "/class/.*": "ClassLoaderResource",
            "/project/.*": "ProjectResource",
            "/display/.*": "ProjectDisplayResource",
            "/resource/.*": "ProjectFileResource",
            "/tree.*": "ProjectTreeResource",
            "/": "ProjectOpenDialog",
            "/.*.js": "TypeScriptResource"
         }
      ]
   },
   "FileMatcher": {
      "class": "develop.http.resource.FileMatcher",
      "constructor-args": [
         "FileResolver",
         "ContentTypeResolver",
         "ConsoleLogger"
      ]
   },
   "CombinationMatcher": {
      "class": "develop.http.resource.CombinationMatcher",
      "constructor-args": [
         [
            "ResourceMatcher",
            "FileMatcher"
         ],
         "NotFoundResource"
      ]
   },
   "ResourceContainer": {
      "class": "develop.http.resource.ResourceContainer",
      "constructor-args": [
         "CombinationMatcher"
      ]
   },
   "WebSocketRouter": {
      "class": "org.simpleframework.http.socket.service.DirectRouter",
      "constructor-args": [
         "ProjectScriptService"
      ]
   },
   "WebSocketContainer": {
      "class": "org.simpleframework.http.socket.service.RouterContainer",
      "constructor-args": [
         "ResourceContainer",
         "WebSocketRouter",
         "2"
      ]
   },
   "WebContainer": {
      "class": "develop.http.WebContainer",
      "constructor-args": [
         "WebSocketContainer",
         "Snap/1.0"
      ]
   },
   "WebServer": {
      "class": "develop.http.WebServer",
      "constructor-args": [
         "WebContainer",
         "${port}"
      ]
   }
}
