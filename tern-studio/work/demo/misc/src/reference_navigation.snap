class Point {
   var x;
   var y;
   new(x,y){
      this.x=x;
      this.y=y;
   }
}
class Line {
   var start;
   var end;
   new(start, end){
      this.start=start;
      this.end=end;
   }
}
var a=new Point(1,2);
var b=new Point(3,4);
var c=new Line(a,null);

println(c?.start?.x);
println(c?.start?.y);
println(c.end?.x);
println(c.end?.y);
