

class GenerateRequest{
   const source;
   const name;
   const success;
   const message;
   const action;
   const key;
   
   new(source, name, success, message, action, key){
      this.source = source;
      this.name = name;
      this.success = success;
      this.message = message;
      this.action = action;
      this.key = key;
   }
   
   isValid(){
      return source != "" && name != "" && action != "";
   }
}
