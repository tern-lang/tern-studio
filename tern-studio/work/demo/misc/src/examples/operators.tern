var x = null;
var y = 11;
var z = 20;

assert true;
assert (2**8) == 256; // power
assert 2==2; // equality with null safe equals()
assert 2===2; // identity equality
assert (2!==2)==false; // identiy inequality
assert 2!==3; 
assert 2**8!==256; // not identity due to boxing
assert 2**8==256;
assert null==x; 
assert null===x; 
assert (null===x??y) == false; // null coalesce 
assert 11===x??y; 
assert 8%2 == 0; // modulus
assert 7%2 == 1;
assert x??y==11;
assert x ?? z == z;
assert x ?? z === z;
assert z << 2 == 80; // bitwise operator
assert "test" == "test";
assert "test" === "test";

