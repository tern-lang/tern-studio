var prefix = "i=";
var t1 = new Thread( ->{
   for(var i in 0..9){
      println("t1:"+prefix+i);
   }
});
t1.start();
t1.join();

var t2 = new Thread(() ->{
   for(var i in 0..9){
      println("t2:"+prefix+i);
   } 
});
t2.start();
t2.join();
