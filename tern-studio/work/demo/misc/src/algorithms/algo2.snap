
func fib(n) {
   if (n<2) {
      return 1;
   }
   return fib(n-1) + fib(n-2);
}

for(var x in 0..4){
   var s = System.currentTimeMillis();
   var r = fib(30);
   var f = System.currentTimeMillis();
   println(f-s);
   println(r);
}

time(() -> fib(30));

