class X{
   var x;
   new(x){
      this.x=x;
   }
}
var a = new X('a');
var b = new X('b');
var c = new X('c');
var l = [a,b,c];

for(var i in l){
   System.err.println(i.x);
}
var arr = new X[10];

for(var i = 0; i < 10; i++){
   arr[i] = new X("i="+i);
}
for(var i in arr){
   System.err.println(i.x);
}

module Blah{
   class Foo{
      var x;
      new(x){
         this.x=x;
      }
   }
}

var f = new Blah.Foo(11);
println(f.x);

fun(f);
fun("f");

function fun(x: Blah.Foo){
   println(x.x);
}
function fun(x: String){
   println("x");
}
