
const numerals = {
   'I': 1,
   'V': 5,
   'X': 10,
   'L': 50,
   'C': 100,
   'D': 500,
   'M': 1000
};

let value = romanToInteger();

func romanToInteger(text) {
   let result = 0;
   
   for(i in 0 .. text.length - 1){
      let key = text.charAt(i);
      let number = numerals[key];
      
      result += number;
      
   }
}